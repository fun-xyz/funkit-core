{
    "name": "fun-wallet",
    "version": "0.2.5",
    "description": "Fun Wallet provides modular access control functionality by leveraging account abstraction",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "test": "env REMOTE_TEST=true mocha test/fork",
        "test-goerli": "env PREFUND=false ts-mocha tests/goerli/*.ts",
        "test-localfork": "env REMOTE_TEST=false mocha test/fork",
        "test-funtestnet": "env REMOTE_TEST=true mocha test/fork",
        "load": "node test/loader.js",
        "format": "prettier --write .",
        "lint": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}'",
        "lint:fix": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}' --fix",
        "prepare": "husky install"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.315.0",
        "@lifeomic/attempt": "^3.0.3",

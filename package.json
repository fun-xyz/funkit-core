{
    "name": "fun-wallet",
    "version": "0.2.5",
    "description": "Fun Wallet provides modular access control functionality by leveraging account abstraction",
    "scripts": {
        "build": "tsc",
        "postinstall": "tsc",
        "test": "env REMOTE_TEST=true mocha test/fork",
        "test-aws": "env PREFUND=false ts-mocha",
        "test-goerli": "env PREFUND=false ts-mocha tests/goerli/*.ts",
        "load": "node test/loader.js",
        "format": "prettier --write .",
        "lint": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}'",
        "lint:fix": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}' --fix",
        "prepare": "husky install"
    },
    "author": "",
    "license": "ISC",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./actions": {
            "types": "./dist/actions.d.ts",
            "default": "./dist/actions.js"
        },
        "./apis": {
            "types": "./dist/apis.d.ts",
            "default": "./dist/apis.js"
        },
        "./auth": {
            "types": "./dist/auth.d.ts",
            "default": "./dist/auth.js"
        },
        "./common": {
            "types": "./dist/common.d.ts",
            "default": "./dist/common.js"
        },
        "./config": {
            "types": "./dist/config.d.ts",
            "default": "./dist/config.js"
        },
        "./data": {
            "types": "./dist/data.d.ts",
            "default": "./dist/data.js"
        },
        "./errors": {
            "types": "./dist/errors.d.ts",
            "default": "./dist/errors.js"
        },
        "./managers": {
            "types": "./dist/managers.d.ts",
            "default": "./dist/managers.js"
        },
        "./servers": {
            "types": "./dist/servers.d.ts",
            "default": "./dist/servers.js"
        },
        "./sponsors": {
            "types": "./dist/sponsors.d.ts",
            "default": "./dist/sponsors.js"
        },
        "./utils": {
            "types": "./dist/utils.d.ts",
            "default": "./dist/utils.js"
        },
        "./wallet": {
            "types": "./dist/wallet.d.ts",
            "default": "./dist/wallet.js"
        }
    },
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.315.0",
        "@lifeomic/attempt": "^3.0.3",
        "@uniswap/sdk": "^3.0.3",
        "@uniswap/v3-core": "^1.0.1",
        "@uniswap/v3-sdk": "^3.9.0",
        "dotenv": "^16.0.3",
        "ethers": "5.7.2",
        "node-fetch": "2.0.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/mocha": "^10.0.1",
        "@types/node-fetch": "^2.6.4",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "chai": "^4.3.7",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TheFunGroup/wallet-sdk-v1.git"
    },
    "bugs": {
        "url": "https://github.com/TheFunGroup/wallet-sdk-v1/issues"
    },
    "homepage": "https://github.com/TheFunGroup/wallet-sdk-v1#readme"
}
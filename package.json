{
    "name": "fun-wallet",
    "version": "0.2.7",
    "description": "Fun Wallet provides modular access control functionality by leveraging account abstraction",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "test": "env REMOTE_TEST=true mocha test/fork",
        "test-aws": "env PREFUND=false ts-mocha",
        "test-goerli": "env PREFUND=false ts-mocha tests/goerli/*.ts",
        "load": "node test/loader.js",
        "format": "prettier --write .",
        "lint": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}'",
        "lint:fix": "eslint -c .eslintrc.json --ext .ts,.tsx './**/*.{ts,tsx}' --fix",
        "prepare": "husky install",
        "postinstall": "npx ts-node tests/contractLoader.ts"
    },
    "author": "The Fun Group (https://fun.xyz/)",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.315.0",
        "@ethersproject/contracts": "^5.7.0",
        "@lifeomic/attempt": "^3.0.3",
        "@uniswap/sdk": "^3.0.3",
        "@uniswap/v3-core": "^1.0.1",
        "@uniswap/v3-sdk": "^3.9.0",
        "dotenv": "^16.0.3",
        "ethers": "5.7.2",
        "node-fetch": "2.0.0",
        "uuid": "^9.0.0",
        "viem": "^0.3.50"
    },
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/mocha": "^10.0.1",
        "@types/node-fetch": "^2.6.4",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "chai": "^4.3.7",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TheFunGroup/fun-wallet-sdk.git"
    },
    "keywords": [
        "Fun Wallet",
        "Ethereum",
        "Account Abstraction",
        "SDK"
    ],
    "files": [
        "dist/src",
        "dist/index.d.ts",
        "dist/index.js",
        "dist/index.js.map"
    ],
    "bugs": {
        "url": "https://github.com/TheFunGroup/wallet-sdk-v1/issues"
    },
    "homepage": "https://github.com/TheFunGroup/wallet-sdk-v1#readme"
}